  "type": "module",
  "author": {
    "name": "Sam Hunt",
    "url": "https://samhunt.dev"
  },
  "homepage": "https://sam-hunt.github.io/school-viewer",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
